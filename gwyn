        local Anim = humanoid:LoadAnimation(AnimAnim)
 
        local startTime = 0
        Anim:Play()
        Anim:AdjustSpeed(1)
        Anim.TimePosition = startTime
        Anim:AdjustSpeed(1)
 
        wait(1)
        Anim:Stop()
    end
end
 
